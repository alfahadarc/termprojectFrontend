<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="460.0" prefWidth="700.0" style="-fx-background-color: #9e9e9e;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.manufacturer.AddCarController">
   <children>
      <AnchorPane layoutX="67.0" layoutY="140.0" prefHeight="200.0" prefWidth="227.0" style="-fx-background-color: #E8E9E8;">
         <children>
            <ImageView fx:id="img_btn" fitHeight="200.0" fitWidth="234.0" onMouseClicked="#open_file" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
      <Label layoutX="105.0" layoutY="357.0" text="Upload Car Image">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label layoutX="340.0" layoutY="100.0" text="Registration Number" />
      <Label layoutX="371.0" layoutY="35.0" text="Add A New Car">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <Label layoutX="390.0" layoutY="132.0" text="Year Made" />
      <Label layoutX="390.0" layoutY="161.0" text="Colour1" />
      <Label layoutX="390.0" layoutY="193.0" text="Colour2" />
      <Label layoutX="390.0" layoutY="222.0" text="Colour3" />
      <Label layoutX="390.0" layoutY="254.0" text="Car Make" />
      <Label layoutX="390.0" layoutY="288.0" text="Car Model" />
      <Label layoutX="390.0" layoutY="323.0" text="Price" />
      <Label layoutX="390.0" layoutY="359.0" text="Quantity" />
      <TextField fx:id="reg_id" layoutX="469.0" layoutY="96.0" />
      <TextField fx:id="year_made" layoutX="469.0" layoutY="128.0" />
      <TextField fx:id="colour_1" layoutX="469.0" layoutY="157.0" />
      <TextField fx:id="colour_2" layoutX="469.0" layoutY="189.0" />
      <TextField fx:id="colour_3" layoutX="469.0" layoutY="218.0" />
      <TextField fx:id="car_make" layoutX="469.0" layoutY="250.0" />
      <TextField fx:id="car_model" layoutX="469.0" layoutY="284.0" />
      <TextField fx:id="price" layoutX="469.0" layoutY="319.0" />
      <TextField fx:id="quantity" layoutX="469.0" layoutY="355.0" />
      <Button fx:id="saveBtn" layoutX="503.0" layoutY="402.0" mnemonicParsing="false" onAction="#save" text="Save" textFill="#2a05ff">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="returnBtn" layoutX="587.0" layoutY="47.0" mnemonicParsing="false" onAction="#backToMain" text="Return" textFill="#0327ff">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
