<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="543.0" prefWidth="703.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.viewer.ViewAllCarsController">
   <children>
      <Label layoutX="291.0" layoutY="27.0" text="All Cars">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="carsTable" layoutX="14.0" layoutY="85.0" prefHeight="447.0" prefWidth="675.0">
        <columns>
          <TableColumn fx:id="reg_num" prefWidth="75.0" text="RegNum" />
          <TableColumn fx:id="year_made" prefWidth="75.0" text="yearMade" />
            <TableColumn fx:id="colour_1" prefWidth="75.0" text="colour1" />
            <TableColumn fx:id="colour_2" prefWidth="75.0" text="colour2" />
            <TableColumn fx:id="colour_3" prefWidth="75.0" text="colour3" />
            <TableColumn fx:id="make" prefWidth="75.0" text="make" />
            <TableColumn fx:id="model" prefWidth="75.0" text="model" />
            <TableColumn fx:id="price" prefWidth="75.0" text="price" />
            <TableColumn fx:id="quantity" prefWidth="75.0" text="quantity" />
        </columns>
      </TableView>
      <Button fx:id="returnBtn" layoutX="628.0" layoutY="37.0" mnemonicParsing="false" onAction="#backToMain" text="Return">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
